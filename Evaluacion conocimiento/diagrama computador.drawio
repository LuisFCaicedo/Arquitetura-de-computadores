        </mxCell>
        <mxCell id="bqwH7B79G_NROaB3eO3u-25" value="Muestra gráficamente todos los proceso que se están ejecutando en el equipo, esta conecta con la tarjeta de video" style="whiteSpace=wrap;html=1;shape=mxgraph.basic.roundRectCallout;dx=30;dy=15;size=5;boundedLbl=1;" vertex="1" parent="1">
          <mxGeometry x="200" y="350" width="139.2" height="110" as="geometry" />
        </mxCell>
        <mxCell id="bqwH7B79G_NROaB3eO3u-26" value="compuesta por letras, símbolos, números etc que sirve para escribir utilizando un sistema de puntadas&amp;nbsp;" style="whiteSpace=wrap;html=1;shape=mxgraph.basic.roundRectCallout;dx=30;dy=15;size=5;boundedLbl=1;" vertex="1" parent="1">
          <mxGeometry x="370" y="354" width="139.2" height="110" as="geometry" />
        </mxCell>
        <mxCell id="bqwH7B79G_NROaB3eO3u-31" value="yo lo defino como el corazon., placa madre del computador por en ella se conectan todos los componetes internos como externos, para coordianarlos y funciones de forma correcta" style="whiteSpace=wrap;html=1;shape=mxgraph.basic.roundRectCallout;dx=30;dy=15;size=5;boundedLbl=1;" vertex="1" parent="1">
          <mxGeometry x="560" y="350" width="200" height="110" as="geometry" />
        </mxCell>
        <mxCell id="bqwH7B79G_NROaB3eO3u-33" value="almacena de forma temporal todos los datos que se ejecutan por parte de los programas que contiene el equipo" style="whiteSpace=wrap;html=1;shape=mxgraph.basic.roundRectCallout;dx=30;dy=15;size=5;boundedLbl=1;" vertex="1" parent="1">
          <mxGeometry x="780" y="350" width="200" height="110" as="geometry" />
        </mxCell>
        <mxCell id="bqwH7B79G_NROaB3eO3u-35" value="Dispositivo de almacenamiento de datos no volátil, donde se guardan todos archivos y el sistema operativo del equipo" style="whiteSpace=wrap;html=1;shape=mxgraph.basic.roundRectCallout;dx=30;dy=15;size=5;boundedLbl=1;" vertex="1" parent="1">
          <mxGeometry x="1000" y="348" width="139.2" height="110" as="geometry" />
        </mxCell>
        <mxCell id="bqwH7B79G_NROaB3eO3u-36" value="El procesador como su nombre lo indica procesa todas las ordenas que ingresan al equipo, organiza o enruta de forma automática y con velocidad todas las aplicaciones intadas&amp;nbsp; que se estan ejecutando&amp;nbsp;" style="whiteSpace=wrap;html=1;shape=mxgraph.basic.roundRectCallout;dx=30;dy=15;size=5;boundedLbl=1;" vertex="1" parent="1">
          <mxGeometry x="1150.8" y="350" width="179.2" height="110" as="geometry" />
        </mxCell>
        <mxCell id="bqwH7B79G_NROaB3eO3u-37" value="La ROM es la memoria de solo lectura, principalmente almacena el Firmware y datos importantes para el funcionamiento del equipo." style="whiteSpace=wrap;html=1;shape=mxgraph.basic.roundRectCallout;dx=30;dy=15;size=5;boundedLbl=1;" vertex="1" parent="1">
