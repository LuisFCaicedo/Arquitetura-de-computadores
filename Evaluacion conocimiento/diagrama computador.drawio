        </mxCell>
        <mxCell id="bqwH7B79G_NROaB3eO3u-22" value="permite diridir el movmiento en la pantalla para trasmitir ordenes por medio de la pantalla" style="whiteSpace=wrap;html=1;shape=mxgraph.basic.roundRectCallout;dx=30;dy=15;size=5;boundedLbl=1;" vertex="1" parent="1">
